  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node ./node_modules/typeorm/cli.js -d ./dist/config/typeorm.config.js",
    "migration": "cd ./src/database/migrations && typeorm migration:create",
    "migration:run": "yarn typeorm migration:run",
    "migration:show": "yarn typeorm migration:show",
    "migration:revert": "yarn typeorm migration:revert",
    "seed:run": "node node_modules/typeorm-extension/bin/cli.cjs seed:run -d dist/config/typeorm.config.js",
    "seed:class": "node node_modules/typeorm-extension/bin/cli.cjs seed:run -d dist/config/typeorm.config.js --name ",
    "generate:secret": "node -e \"console.log(require('crypto').randomBytes(256).toString('base64'));\""
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": "please-use-yarn"
  },
